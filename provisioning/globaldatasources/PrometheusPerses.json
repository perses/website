{
  "kind": "GlobalDatasource",
  "metadata": {
    "name": "PrometheusPerses"
  },
  "spec": {
    "display": {
      "name": "Prometheus Perses",
      "description": "A Prometheus demo instance hosted by Perses org"
    },
    "default": true,
    "plugin": {
      "kind": "PrometheusDatasource",
      "spec": {
        "proxy": {
          "kind": "HTTPProxy",
          "spec": {
            "url": "http://prometheus:9090"
          }
        }
      }
    }
  }
}
