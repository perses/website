  "kind": "GlobalDatasource",
  "metadata": {
    "name": "PrometheusLocal"
  },
  "spec": {
    "display": {
      "name": "Prometheus Local",
      "description": "Your locally-hosted Prometheus instance for testing"
    },
    "default": false,
    "plugin": {
      "kind": "PrometheusDatasource",
